@model DBL.Entities.Systemproperty
<div class="modal-header">
    <h5 class="text-sm modal-title" id="Uttambsolutionsmodallabel">Property Details</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <div class="row">
        <input class="form-control form-control-sm" asp-for="Propertyid" id="Propertyid" hidden />
    </div>
    <div class="row">
        <div class="col-sm-12">
            <div class="form-group">
                <label class="font-weight-light">House Name</label>
                <input class="form-control form-control-sm" asp-for="Propertyname" id="Rolenameid" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-4">
            <div class="form-group">
                <label class="font-weight-light">Town</label>
                <select class="form-control form-control-sm select2" id="JobEmployerId" required>
                    <option value="">---Select---</option>
                </select>
            </div>
        </div>
        <div class="col-sm-4">
            <div class="form-group">
                <label class="font-weight-light">Town</label>
                <select class="form-control form-control-sm select2" id="JobEmployerId" required>
                    <option value="">---Select---</option>
                </select>
            </div>
        </div>
        <div class="col-sm-4">
            <div class="form-group">
                <label class="font-weight-light">Town</label>
                <select class="form-control form-control-sm select2" id="JobEmployerId" required>
                    <option value="">---Select---</option>
                </select>
            </div>
        </div>
    </div>
    <h6 class="card-title text-sm">Rooms Available</h6>
    <br />
    <div class="container">
        <div class="row align-items-end">
            <div class="col-sm-4 mb-2">
                <div class="form-group">
                    <label class="text-sm font-weight-light fw-light">Room Size</label>
                    <select class="form-control form-control-sm select2" id="JobEmployerId" required>
                        <option value="">---Select---</option>
                        <option value="Single">Single</option>
                        <option value="Double">Double</option>
                        <option value="Bed Sitter">Bed Sitter</option>
                        <option value="One Bedroom">One Bedroom</option>
                        <option value="Two Bedroom">Two Bedroom</option>
                    </select>
                </div>
            </div>
            <div class="col-sm-3 mb-2">
                <div class="form-group">
                    <label class="text-sm font-weight-light fw-light">How Many</label>
                    <select class="form-control form-control-sm select2" id="numberSelect">
                        <option value="">---Select---</option>
                        <option value="1">1</option>
                        <option value="2">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                        <option value="5">5</option>
                        <option value="6">6</option>
                        <option value="7">7</option>
                        <option value="8">8</option>
                        <option value="9">9</option>
                        <option value="10">10</option>
                        <option value="11">11</option>
                        <option value="12">12</option>
                        <option value="13">13</option>
                        <option value="14">14</option>
                        <option value="15">15</option>
                        <option value="16">16</option>
                        <option value="17">17</option>
                        <option value="18">18</option>
                        <option value="19">19</option>
                        <option value="20">20</option>
                    </select>
                </div>
            </div>
            <div class="col-sm-3 mb-2">
                <div class="form-group">
                    <label class="text-sm font-weight-light fw-light">Rent Amount</label>
                    <input type="text" class="form-control form-control-sm" id="SystemjobfunctionId2" placeholder="Rent Amount" aria-label="Rentamount" />
                </div>
            </div>
            <div class="col-sm-2 mb-4">
                <button class="btn btn-sm btn-secondary w-100" onclick="addsystemjobfunctiondata()">Add</button>
            </div>
        </div>
    </div>
    <div class="table-responsive">
        <table class="table table-bordered table-striped table-sm text-nowrap w-100" id="AllsystemDataTables">
            <thead>
                <tr>
                    <th hidden>Responsibilityid</th>
                    <th class="font-weight-light fw-light">Responsibility</th>
                    <th class="font-weight-light fw-light text-right">Action</th>
                </tr>
            </thead>
            <tbody>
               @*  @if (Model.Systemjobfunction != null)
                {
                    foreach (var item in Model.Systemjobfunction)
                    {
                        <tr>
                            <td hidden>@item.Jobfunctionid</td>
                            <td>@item.Jobfunction</td>
                            <td class="text-right">
                                <button class="btn-info btn-xs btn-outline-info text-white font-weight-bold text-uppercase m-1" onclick="editjobfunctiondata(this.parentNode.parentNode)">Edit</button>
                                <button class="btn-info btn-xs btn-outline-info text-white font-weight-bold text-uppercase m-1" onclick="updatejobfunctiondata(this.parentNode.parentNode)">Update</button>
                            </td>
                        </tr>
                    }
                } *@
            </tbody>
        </table>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-sm btn-danger" data-dismiss="modal">Close</button>
    <button class="btn btn-sm btn-info font-weight-bold" type="button" id="savesystemroleid" onclick="savesystemroledata();">
        <span id="buttonText">SAVE</span><span id="processingSpinner" class="spinner-grow spinner-grow-sm" role="status" aria-hidden="true" style="display: none;"></span>
    </button>
</div>
