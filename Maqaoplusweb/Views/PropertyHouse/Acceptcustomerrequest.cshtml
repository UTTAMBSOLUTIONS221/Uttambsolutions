@model DBL.Entities.SystemPropertyHouseVacatingRequest
<div class="modal-header">
    <h5 class="text-sm modal-title" id="Uttambsolutionsmodallabel">Validate Invoice Payyment</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <div class="row">
        <input class="form-control form-control-sm" asp-for="@Model.Vacatingrequestid" id="Systempropertyhousevacatingrequestid" hidden />
        <input class="form-control form-control-sm" asp-for="@Model.Tenantid" id="Systempropertyhousetenantid" hidden />
        <input class="form-control form-control-sm" asp-for="@Model.Tenantid" id="Systempropertyhouseroomid" hidden />
    </div>
    <div class="row">
        <div class="col-sm-12">
            <select name="ratingValue" id="ratingValue">
                @for (int i = 1; i <= 5; i++)
                {
                    <option value="@i">@i</option>
                }
            </select>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-12">
            <label for="comment">Comment:</label>
            <textarea name="comment" id="comment"></textarea>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-sm btn-danger" data-dismiss="modal">Close</button>
    <button class="btn btn-sm btn-info font-weight-bold" type="button" id="savesystempropertyroompaymentvalidationid" onclick="savesystempropertyroompaymentvalidationdata();">
        <span id="buttonText">SAVE</span><span id="processingSpinner" class="spinner-grow spinner-grow-sm" role="status" aria-hidden="true" style="display: none;"></span>
    </button>
</div>