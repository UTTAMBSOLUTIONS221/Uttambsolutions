@model DBL.Entities.SystemPropertyHouseVacatingRequest

<div class="modal-header">
    <h5 class="text-sm modal-title" id="Uttambsolutionsmodallabel">Accept the vacating Request</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <div class="row">
        <input class="form-control form-control-sm" asp-for="@Model.Vacatingrequestid" id="Systempropertyhousevacatingrequestid" hidden />
        <input class="form-control form-control-sm" asp-for="@Model.Systempropertyhousetenantid" id="Systempropertyhousetenantid" hidden />
        <input class="form-control form-control-sm" asp-for="@Model.Systempropertyhouseroomid" id="Systempropertyhouseroomid" hidden />
    </div>
    <div class="form-group d-flex align-items-center">
        <label for="rating" class="mr-2">Rating:</label>
        <div class="rateYo"></div>
        <input type="hidden" class="rating-value" name="rating" id="Systempropertyhouseroomtenantratingid" value="0">
    </div>

    <div class="row">
        <div class="col-sm-12">
            <label for="comment">Comment:</label>
            <textarea name="comment" id="Systempropertyhouseroomtenantcommentid" class="form-control form-control-sm"></textarea>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-sm btn-danger" data-dismiss="modal">Close</button>
    <button class="btn btn-sm btn-info font-weight-bold" type="button" id="Acceptsystempropertyroomvacatingrequestid" onclick="Acceptsystempropertyroomvacatingrequestdata();">
        <span id="buttonText">SAVE</span><span id="processingSpinner" class="spinner-grow spinner-grow-sm" role="status" aria-hidden="true" style="display: none;"></span>
    </button>
</div>
