@model DBL.Entities.Parcelcollectioncenters
<div class="modal-header">
    <h5 class="text-sm modal-title" id="Uttambsolutionsmodallabel">Collection Detail</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<div class="modal-body">
    <div class="row">
        <div class="col-sm-12">
            <input asp-for="@Model.Collectioncenterid" id="Collectioncenterid" hidden />
        </div>
    </div>
    <div class="row">
        <div class="col-sm-5">
            <div class="form-group">
                <label class="font-weight-light">Name</label>
                <input class="form-control form-control-sm" asp-for="@Model.Collectionname" id="Collectionnameid" />
            </div>
        </div>
        <div class="col-sm-7">
            <div class="form-group">
                <label class="font-weight-light">Address</label>
                <input class="form-control form-control-sm" asp-for="@Model.Collectionaddress" id="Collectionaddressid" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-5">
            <div class="form-group">
                <label class="font-weight-light">Phone Number</label>
                <input class="form-control form-control-sm" asp-for="@Model.Phonenumber" id="Phonenumberid" />
            </div>
        </div>
        <div class="col-sm-7">
            <div class="form-group">
                <label class="font-weight-light">Operating Hours</label>
                <input class="form-control form-control-sm" asp-for="@Model.Operatinghours" id="Operatinghoursid" />
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-sm btn-danger" data-dismiss="modal">Close</button>
    <button class="btn btn-sm btn-info font-weight-bold" type="button" id="saveparcelcollectioncenterid" onclick="saveparcelcollectioncenterdata();">
        <span id="buttonText">SAVE</span><span id="processingSpinner" class="spinner-grow spinner-grow-sm" role="status" aria-hidden="true" style="display: none;"></span>
    </button>
</div>
